Move  (direction, index) { \r\n           \r\n        const newNotes = Array.from(notes)\r\n        // removeItemNode recebe o retorno do metodo splice que é um array com os elementos removidos  \r\n        const removeItemNode = newNotes.splice(index, 1)[0];       \r\n        if(direction === \"up\"){\r\n          newNotes.splice(index -1, 0, removeItemNode);  \r\n        }else{\r\n            newNotes.splice(index +1, 0, removeItemNode);   \r\n        }         \r\n        setNotes(newNotes)\r\n    }\r\n\r\n// ----------------------------------------------------------------------------------\r\n    function handleDelete(id) {        \r\n            const newNotes = Array.from(notes);\r\n            const index = newNotes.findIndex(note => note.id === id);\r\n            newNotes.splice(index, 1);       \r\n            setNotes(newNotes)\r\n        }\r\n\r\n    // ----------------------------------------------------------------------------\r\n    function handleEdit  (id, text){        \r\n            const newNotes = notes.slice();\r\n            const index = newNotes.findIndex(note => note.id === id);\r\n            newNotes[index].text = text;    \r\n            setNotes(newNotes)           \r\n        }\r\n    // --------------------------------------------------------------------------------------  \r\n        return(\r\n            <div className=\"container\">\r\n                <NewNote onAddNote = {handleAddNote}/>\r\n                <NoteList\r\n                notes={notes}\r\n                onMove={handleMove}\r\n                onDelete={handleDelete}\r\n                onEdit={handleEdit}\r\n                />\r\n            </div>\r\n        )\r\n  \r\n}  \r\n\r\n export default App;"},"lineCount":null}},"error":null,"hash":"307bcb3e90cca777293bf62fd67c64ad","cacheData":{"env":{}}}