rt React from \"react\";\r\nimport ReactDOM from \"react-dom\";\r\n\r\nimport \"./index.scss\";\r\n\r\nclass App extends React.Component {\r\n    state = {\r\n        notes: []\r\n    };\r\n\r\n    handleAddNote = (texto) => {\r\n        this.setState(prevState =>( {\r\n            notes: prevState.notes.concat(texto) }));\r\n    }\r\n\r\n    render(){\r\n        return(\r\n            <div className=\"container\">\r\n                <NewNote onAddNote = {this.handleAddNote}/>\r\n                <NoteList  notes={this.state.notes}/>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nclass NewNote extends React.Component{\r\n    state = {\r\n        text:\"\"\r\n    }\r\n    render(){\r\n        const {onAddNote} = this.props;\r\n        const { text} = this.state;\r\n        return (\r\n            <div className=\"caixaNote\">\r\n            <input type=\"text\" className=\"note\"\r\n             placeholder=\"digite sua nota\"\r\n             value ={text}\r\n             onChange={ event => {\r\n                 this.setState({\r\n                     text: event.target.value\r\n                 })\r\n             }}\r\n             onKeyPress = { event => {\r\n                if(event.key === \"Enter\"){\r\n                    onAddNote(event.target.value);\r\n                    this.setState({text:\"\"});\r\n                }\r\n            }}\r\n            />\r\n        </div>\r\n        )\r\n    }\r\n}\r\n \r\n\r\nconst NoteList = ({notes}) =>(\r\n    <div className=\"lista\">\r\n    {notes.map(note => <div className=\"item\">{note}</div>)}               \r\n    </div>\r\n)\r\n\r\nReactDOM.render(\r\n <App />,\r\n document.getElementById(\"root\")\r\n\r\n)\r\n"},"lineCount":null}},"error":null,"hash":"d54ab2182ab398847ffacdfcbf92ef26","cacheData":{"env":{}}}